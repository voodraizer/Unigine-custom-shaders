<!-- Custom mesh material -->
<?xml version="1.0" encoding="utf-8"?>
<base_material version="2.11.0.0"  name="base_mesh_vblend" editable="0" parameters_prefix="m" defines="VERTEX_ATTRIBUTE_GEOMETRY" default="1">
	
	<state name="ambient_light" internal="1">1</state>
	
	<state name="alpha_fade"	defines="name"	internal="1"/>
	<state name="spline"		defines="name"	internal="1"/>
	<state name="wireframe_antialiasing"	defines="name"	internal="1"	pass_defines="wireframe"/>
	
	<state name="dynamic"		vegetation="0"											defines="name"	internal="1"/>
	<state name="static_shadow"	vegetation="0"	parallax="0"	geometry_inflation="0"	defines="name"	internal="1"/>
	
	<group name="Default">
		<state name="workflow" items="metalness,specular"			defines="items"	tooltip="&lt;p&gt;Workflow selection determines which textures are to be used.&lt;/p&gt;&lt;p&gt;There are two workflows available:&lt;ul&gt;&lt;li&gt;&lt;b&gt;Metalness&lt;/b&gt; — new standard of textures (albedo, metalness, roughness).&lt;/li&gt;&lt;li&gt;&lt;b&gt;Specular&lt;/b&gt; — old standard of textures (diffuse, specular, gloss).&lt;/li&gt;&lt;/ul&gt;&lt;/p&gt;&lt;p&gt;It is strongly recommended to use the Metalness workflow as it uses a less number of texture channels, and uses material parameters more similar to real ones.&lt;/p&gt;"/>
		<state name="deferred" title="Deferred Buffers"								tooltip="&lt;p&gt;Enable filling of deferred buffers.&lt;/p&gt;&lt;p&gt;Used by transparent objects to participate in post-effects.&lt;/p&gt;">1</state>
		<state name="multiple_environment_probes" transparent="2"	defines="name"	tooltip="&lt;p&gt;Illuminate a transparent object with several environment probes and map their cubemaps to this object.&lt;/p&gt;">0</state>
		<state name="terrain_lerp" defines="name"/>
	</group>
	
	<group name="Options">
		<state name="ao_map"								defines="name"	pass_defines="deferred,forward"	title="Ambient Occlusion"	tooltip="&lt;p&gt;Enable the Ambient Occlusion effect.&lt;/p&gt;&lt;p&gt;This effect is used to modulate global environment illumination using baked shadows from environment probe light sources.&lt;/p&gt;"/>
		<state name="specular_map"		workflow="0"		defines="name"	pass_defines="deferred,forward"								tooltip="&lt;p&gt;Use B-channel of the &lt;b&gt;Shading Texture&lt;/b&gt; as a specular reflection intensity map.&lt;/p&gt;"/>
		<state name="microfiber_map"						defines="name"	pass_defines="deferred,forward"								tooltip="&lt;p&gt;Use A-channel of the &lt;b&gt;Shading Texture&lt;/b&gt; as a microfiber map.&lt;/p&gt;"/>
		<state name="translucent_map"	transparent="0,1"	defines="name"	pass_defines="deferred,forward"								tooltip="&lt;p&gt;Enable the translucency effect.&lt;/p&gt;&lt;p&gt;This effect is used to create translucent materials, that allow light to pass through.&lt;/p&gt;"/>
		
		<state name="detail"	defines="name"	pass_defines="deferred,transparent"	tooltip="&lt;p&gt;Enable detail rendering.&lt;/p&gt;&lt;p&gt;This effect can be used to add details for objects, that are close to camera, or variations for distant ones, to make them look more realistic.&lt;/p&gt;"/>
		
		<state name="emission"			defines="name"	pass_defines="ambient,deferred"								tooltip="&lt;p&gt;Enable emission rendering.&lt;/p&gt;&lt;p&gt;This effect simulates glow from extremely bright surfaces and can be used to create self-luminous objects (lamps, fire, etc.)&lt;/p&gt;"/>
		<state name="emission_bake_gi"	defines="name"	pass_defines="ambient,emission"				emission="1"	tooltip="">1</state>
		<state name="cross_section"		defines="name"	pass_defines="deferred,ambient,emission"	emission="1"	tooltip=""/>
		
		<state name="lightmap"				defines="name"	pass_defines="deferred,ambient"											tooltip="&lt;p&gt;Enable use of a lightmap.&lt;/p&gt;&lt;p&gt;Activates an additional light map texture specifying that the material will be lit by it.&lt;/p&gt;"/>
		<state name="procedural_effects"	defines="name"	pass_defines="deferred"													tooltip="&lt;p&gt;Enable use of procedural effects: SSBevel and SSDirt.&lt;/p&gt;"/>
		<state name="bevel"					defines="name"	pass_defines="deferred"			procedural_effects="1" title="SSBevel"	tooltip="&lt;p&gt;Enable the SSBevel effect.&lt;/p&gt;&lt;p&gt;This effect allows you to create smoothed edges of geometry and gives a realistic look to objects in the scene.&lt;/p&gt;"/>
		<state name="curvature"				defines="name"	pass_defines="deferred"			procedural_effects="1" title="SSDirt"	tooltip="&lt;p&gt;Enable the SSDirt effect.&lt;/p&gt;&lt;p&gt;This effect allows you to simulate worn scratched edges and accumulation of dirt and dust in cavities in the screen space globally. It saves time for large complex scenes with a lot of various models and elements making them look more natural without having to prepare textures for each of them individually.&lt;/p&gt;"/>
		<state name="auxiliary"				defines="name"	pass_defines="auxiliary"												tooltip="&lt;p&gt;Enable auxiliary rendering pass for the material.&lt;/p&gt;&lt;p&gt;Can be used for custom post-effects, such as thermal vision, etc.&lt;/p&gt;"/>
		
		<state name="parallax"	transparent="0,1"	defines="name"	pass_defines="deferred,emission,shadow,depth_pre_pass"	tooltip="&lt;p&gt;Enable parallax occlusion mapping.&lt;/p&gt;&lt;p&gt;Can be used to fake surface relief adding more realism with less influence on the performance.&lt;/p&gt;"/>
		
		<state name="vertex_color"		defines="name"	pass_defines="deferred,transparent,emission"	tooltip="&lt;p&gt;Use vertex colors for per-channel modulation of the albedo, AO, light map, emission, detail and auxiliary textures.&lt;/p&gt;&lt;p&gt;Can be used to reduce the number of textures and broaden the diversity of materials.&lt;/p&gt;"/>
		
		<state name="vertex_blend_enable"	defines="name"	pass_defines="deferred,emission"	tooltip="&lt;p&gt;Use vertex colors for blend textures by vertex color.&lt;/p&gt;&lt;p&gt;Can be used to reduce the number of textures and broaden the diversity of materials.&lt;/p&gt;Mask texture: R-blend texture, G-blend texture, B-dirt texture (multiply).&lt;p&gt;Vertex color: R-blend r-channel, G-blend g-channel, B-blend b-channel.&lt;/p&gt;"/>

		<state name="planar_reflection"	defines="name"	pass_defines="ambient"							tooltip="&lt;p&gt;Enable rendering of dynamic reflections.&lt;/p&gt;&lt;p&gt;Using this option you can create realistic flat mirrors and flat reflective surfaces (parquet, flat varnished surfaces, etc.)&lt;/p&gt;"/>
		<state name="vegetation"		defines="name"													tooltip="&lt;p&gt;Use the material for vegetation simulation.&lt;/p&gt;&lt;p&gt;Enabling this option allows you to create materials for vegetation crowns and trunks, that can be animated to add more realism.&lt;/p&gt;"/>
		
		<state name="jitter_transparency"		defines="name"	pass_defines="object"	transparent="1"	/>
		
		<state name="lightmap_with_ambient"		defines="name"	pass_defines="deferred,ambient"	tooltip="&lt;p&gt;Allows the light map to take ambient lighting into account.&lt;/p&gt;&lt;p&gt;When unchecked, only the light map is rendered.&lt;/p&gt;">0</state>
		
		<state name="mip_bias"					defines="name"	pass_defines="object"	tooltip=""/>
		
		<state name="angle_fade" transparent="1" defines="name" tooltip="&lt;p&gt;Enables cutting out surfaces at a certain angle between the surface normal and the camera view direction vector&lt;/p&gt;&lt;p&gt;This option can be used to gain performance&lt;/p&gt;"/>	
		
		<state name="geometry_inflation"	items=",wire,balloon"				defines="items"			tooltip="&lt;p&gt;Makes it possible to bloat the geometry of objects to make them visible from a distant position.&lt;/p&gt;&lt;p&gt;Can be used for wires and sphere-shaped objects.&lt;/p&gt;"/>
		
		<state name="opacity_map"			transparent="1,2"	items="main,normal"	defines="name_items"	pass_defines="object"	tooltip="&lt;p&gt;Texture source for opacity map: A-channel of the diffuse/albedo texture or B-channel of the normal map&lt;/p&gt;"/>
		
		<state name="refraction"						transparent="2"																			tooltip="&lt;p&gt;Enables rendering of post-processing refraction.&lt;/p&gt;&lt;p&gt;This effect makes transparent object look more realistic by distorting an image behind it.&lt;/p&gt;"/>
		
		<state name="transparent_blur"		transparent="2"											tooltip="&lt;p&gt;Enable rendering of transparent blur effect.&lt;/p&gt;&lt;p&gt;This effect makes it possible to create transparent matte objects (e.g. matte glass).&lt;/p&gt;"/>
	</group>
	
	<group name="Normals">
		<state name="normal_map"				pass_defines="object"	defines="name_items"	items="tangent_space,object_space"/>
		<state name="calculate_tangent_space"	pass_defines="object"	defines="name"			tooltip="&lt;p&gt;Enables calculation of the tangent space in the shader.&lt;/p&gt;&lt;p&gt;Can be used for materials applied to geometry with incorrect tangents or to geometry without tangents at all.&lt;/p&gt;"/>
		<state name="normals_rotated_to_camera"							defines="name"			tooltip="All normals rotated to camera ignoring mesh normal"/>
		<state name="backside_normal"			pass_defines="object"	defines="name_items"	items="to_back_side,to_front_side"	two_sided="1"/>
	</group>

	<group name="Blending">
		<state name="vertex_color_blend_r"		defines="name"	vertex_blend_enable="1"	pass_defines="deferred,emission"	tooltip="&lt;p&gt;Enable blending by vertex R.&lt;/p&gt;"/>
		<state name="vertex_color_blend_g"		defines="name"	vertex_blend_enable="1" pass_defines="deferred,emission"	tooltip="&lt;p&gt;Enable blending by vertex G.&lt;/p&gt;"/>
	</group>
	
	<group name="Detail">
		<state name="detail_blend"		detail="1" pass_defines="deferred,transparent"	items="alpha_blend,overlay,multiply"			defines="name_items"	title="Blending"			tooltip="&lt;p&gt;Blending mode: determines how base and detail textures are blended.&lt;/p&gt;"/>
		<state name="detail_angle_fade"	detail="1" pass_defines="deferred,transparent"	items=",detail_angle_fade0,detail_angle_fade1"	defines="items"			title="Angle Fade"			tooltip="&lt;p&gt;Overlapping mode for base textures:&lt;ul&gt;&lt;li&gt;overlap the base material by using the object transformation (detail textures will be rotated with the mesh).&lt;/li&gt;&lt;li&gt;World transform (overlap) - overlap the base textures by using the world transformation (no matter if you rotate the object, the detail textures will always be projected atop).&lt;/li&gt;&lt;/ul&gt;&lt;/p&gt;"/>
		<state name="detail_mask"		detail="1" pass_defines="deferred,transparent"													defines="name"			title="Mask"				tooltip="&lt;p&gt;Use a texture mask for blending base and detail materials selectively (the detail textures will be shown according to the mask).&lt;/p&gt;"/>
		<state name="normal_substitute"	detail="1" pass_defines="deferred,transparent"													defines="name"			title="Normal Substitute"	tooltip="&lt;p&gt;Rendering mode for normals.&lt;/p&gt;&lt;p&gt;By default, normal maps of a base and detail normal map are combined together. With this option enabled, only detail normals overlaps normals.&lt;/p&gt;">0</state>
	</group>
	
	<group name="UV mapping">
		<state name="base_mapping"										items="uv0,uv1,triplanar,triplanar_world,overlap,overlap_world"				defines="name_items"													title="Base"				tooltip="UV map source for base textures"/>
		<state name="detail_mapping"		detail="1"					items="base,uv0,uv1,triplanar,triplanar_world,overlap,overlap_world"		defines="name_items"	pass_defines="deferred,transparent"				title="Detail"				tooltip="UV map source for detail textures">1</state>
		<state name="detail_mask_mapping"	detail="1"	detail_mask="1"	items="base,detail,uv0,uv1,triplanar,triplanar_world,overlap,overlap_world"	defines="name_items"	pass_defines="deferred,transparent"				title="Detail Mask"			tooltip="UV map source for the detail blend mask texture"/>
		<state name="emission_mapping"		emission="1"				items="base,uv0,uv1,triplanar,triplanar_world,overlap,overlap_world"		defines="name_items"	pass_defines="deferred,transparent,emission"	title="Emission"			tooltip="UV map source for the emission texture"/>
		<state name="ao_mapping"			ao_map="1"					items="base,uv0,uv1"														defines="name_items"	pass_defines="deferred,forward"					title="Ambient Occlusion"	tooltip="UV map source for the ambient occlusion texture">1</state>
		<state name="lightmap_mapping"		lightmap="1"				items="base,uv0,uv1"														defines="name_items"	pass_defines="deferred,ambient"					title="Lightmap"			tooltip="UV map source for the lightmap occlusion texture">2</state>
		<state name="bevel_mapping"			bevel="1"					items="uv0,uv1,triplanar"													defines="name_items"	pass_defines="deferred"							title="SSBevel"				tooltip="UV map source for the SSBevel map">0</state>
	</group>
	
	<group name="Parallax">
		<state name="parallax_depth_cutout"	parallax="1" transparent="0,1" defines="name" pass_defines="deferred,emission,shadow,depth_pre_pass"	title="Depth Cutout"	tooltip="&lt;p&gt;Enable the depth cutout effect providing smooth alignment with real surface geometry.&lt;/p&gt;&lt;p&gt;This effect makes a plane look like real geometry. It also improves the SSAO calculation on surfaces with parallax maps.&lt;/p&gt;"/>
		<state name="parallax_cutout"		parallax="1" transparent="0,1" defines="name" pass_defines="deferred,emission,shadow,depth_pre_pass"	title="Cutout"			tooltip="&lt;p&gt;Enable the cutout effect providing smooth edges between the opaque and transparent areas.&lt;/p&gt;"/>
	</group>
	
	<group name="Vertex Color">
		<state name="vertex_albedo"			vertex_color="1"	workflow="0"	items=",rgb"		defines="name"			pass_defines="deferred,forward"		title="Albedo"				tooltip="Use vertex color for albedo texture"/>
		<state name="vertex_diffuse"		vertex_color="1"	workflow="1"	items=",rgb"		defines="name"			pass_defines="deferred,forward"		title="Diffuse"				tooltip="Use vertex color for diffuse texture"/>
		<state name="vertex_ao"				vertex_color="1"					items=",r,g,b,a"	defines="name_items"	pass_defines="deferred,forward"		title="Ambient Occlusion"	tooltip="Use vertex color for the ambient occlusion texture"/>
		<state name="vertex_lightmap"		vertex_color="1"					items=",rgb"		defines="name"			pass_defines="deferred,ambient"		title="Light Map"			tooltip="Use vertex color for the lightmap texture"/>
		<state name="vertex_detail_mask"	vertex_color="1"	detail="1"		items=",r,g,b,a"	defines="name_items"	pass_defines="deferred,transparent"	title="Detail Mask"			tooltip="Use vertex color for the detail mask texture"/>
		<state name="vertex_emission"		vertex_color="1"	emission="1"	items=",rgb"		defines="name"			pass_defines="emission"				title="Emission"			tooltip="Use vertex color for the emission texture"/>
		<state name="vertex_auxiliary"		vertex_color="1"	auxiliary="1"	items=",rgb"		defines="name"			pass_defines="auxiliary"			title="Auxiliary"			tooltip="Use vertex color for the auxiliary texture"/>
	</group>
	
	<group name="Vegetation">
		<state name="animation"				vegetation="1" items=",default,field"		defines=",ANIMATION,FIELD_ANIMATION"										tooltip="&lt;p&gt;Mode of procedural animation of wind affecting vegetation.&lt;/p&gt;&lt;p&gt;Determines if animation is enabled and controlled by Vegetation animation parameters only, or by a &lt;b&gt;FieldAnimation&lt;/b&gt; node.&lt;/p&gt;"/>
		<state name="leafs"					vegetation="1" items=",geometry_uv_based,billboard,geometry_vertex_color_based"	defines="name_items"		title="Leaves"	tooltip="&lt;p&gt;Leaves rendering mode&lt;/p&gt;&lt;p&gt;Determines the way the geometry of leaves is rendered. Geometry mode provides more realism, but it is less performance-friendly.&lt;/p&gt;"/>
		<state name="shadow_offset_state"	vegetation="1" pass_defines="shadow"		defines=",SHADOW_OFFSET"				title="Shadow Offset"				tooltip="&lt;p&gt;Offset parameter for leaves shadows.&lt;/p&gt;"/>
		<state name="vegetation_velocity"	vegetation="1" title="Vegetation Velocity"	defines=",VEGETATION_VELOCITY" tooltip="&lt;p&gt;Render to velocity buffer&lt;/p&gt;">1</state>
	</group>
	
	<group name="Spatial Color Noise">
		<state name="noise_2d" transparent="0,1" defines="name" pass_defines="deferred" title="2D Noise" tooltip="&lt;p&gt;Add spatial color noise based on 2D texture.&lt;/p&gt;&lt;p&gt;Can be used to add random color variation for leaves.&lt;/p&gt;"/>
		<state name="noise_3d" transparent="0,1" defines="name" pass_defines="deferred" title="3D Noise" tooltip="&lt;p&gt;Add spatial color noise based on 3D texture.&lt;/p&gt;&lt;p&gt;Can be used to add random color variation for leaves.&lt;/p&gt;"/>
	</group>
	
	<group name="Planar Reflection">
		<state name="reflection_size"		planar_reflection="1"	items="128,256,512,1024,2048,4096,quart_height,half_height,height"	title="Map Size"	tooltip="&lt;p&gt;Reflection map size, in pixels.&lt;/p&gt;"			>7</state>
		<state name="reflection_two_sided"	planar_reflection="1"																		title="Two Sided"	tooltip="&lt;p&gt;Enable two-sided planar reflection mode.&lt;/p&gt;"	>0</state>
		<state name="reflection_show_pivot"	planar_reflection="1"																		title="Show Pivot"	tooltip="&lt;p&gt;Show the reflection pivot point.&lt;/p&gt;"			>1</state>
		<state name="reflection_blur"		planar_reflection="1"	pass_defines="reflection"	defines="name"							title="Blur"																				>1</state>
	</group>
	
	<group name="Post Processing">
		<state name="material_ssao"					deferred="1"	transparent="0,1"		title="SSAO"				tooltip="&lt;p&gt;Enable the Screen Space Ambient Occlusion effect.&lt;/p&gt;">1</state>
		<state name="material_ssr"					deferred="1"	transparent="0,1"		title="SSR"					tooltip="&lt;p&gt;Enable the Screen Space Reflections effect.&lt;/p&gt;">1</state>
		<state name="material_sssss"				deferred="1"	transparent="0,1"		title="SSSSS"				tooltip="&lt;p&gt;Enable the Subsurface Scattering effect.&lt;/p&gt;">0</state>
		<state name="material_dof"					deferred="1"							title="DOF"					tooltip="&lt;p&gt;Enable Depth Of Field effect.&lt;/p&gt;">1</state>
		<state name="material_motion_blur"			deferred="1"							title="Motion Blur"			tooltip="&lt;p&gt;Enable the Motion Blur effect.&lt;/p&gt;">1</state>
		<state name="material_sun_shafts"			deferred="1"							title="Sun Shafts"			tooltip="&lt;p&gt;Enable the Sun Shafts effect.&lt;/p&gt;">1</state>
		<state name="material_shoreline_wetness"	deferred="1"	transparent="0,1"		title="Shoreline Wetness"	tooltip="&lt;p&gt;Enable the wetness effect for objects near the shoreline.&lt;/p&gt;">1</state>
	</group>
	
	<group name="Light Passes">
		<state name="ambient"			transparent="2"	title="Ambient"				tooltip="&lt;p&gt;Enable rendering of the material illuminated by an environment probe light source.&lt;/p&gt;"	>1</state>
		<state name="environment_probe"	transparent="2"	title="Environment probe"	tooltip="&lt;p&gt;Enable rendering of the material illuminated by an environment probe light source.&lt;/p&gt;"	>1</state>
		<state name="voxel_probe"		transparent="2"	title="Voxel probe"			tooltip="&lt;p&gt;Enable rendering of the material illuminated by an voxel probe light source.&lt;/p&gt;"		>1</state>
		<state name="light_omni"		transparent="2"	title="Light Omni"			tooltip="&lt;p&gt;Enable rendering of the material illuminated by an omni light source.&lt;/p&gt;"				>1</state>
		<state name="light_proj"		transparent="2"	title="Light Proj"			tooltip="&lt;p&gt;Enable rendering of the material illuminated by a projected light source.&lt;/p&gt;"			>1</state>
		<state name="light_world"		transparent="2"	title="Light World"			tooltip="&lt;p&gt;Enable rendering of the material illuminated by a world light source.&lt;/p&gt;"				>1</state>
	</group>
	
	<group name="Shadows">
		<state name="lerp_cascades"			transparent="2"	title="Lerp Cascades"	pass_defines="forward"			defines=",USE_LERP_CASCADES">0</state>
		<state name="shadow_filter_mode"	transparent="2"	title="Filter Mode"		pass_defines="forward"			defines="name_items"		items="none,low,medium,high,ultra"	tooltip="&lt;p&gt;Filtering mode to be used for shadows from all light sources cast on the material.&lt;/p&gt;&lt;p&gt;This mode determines the quality of soft shadows reducing the stair-step effect.&lt;/p&gt;&lt;p&gt;When disabled no filtering is performed, the stair-step effect is clearly seen at the edges of shadows.&lt;/p&gt;&lt;p&gt;&lt;em&gt;Higher&lt;/em&gt; quality produces &lt;em&gt;smoother&lt;/em&gt; shadow edges.&lt;/p&gt;&lt;p&gt;&lt;b&gt;Notice&lt;/b&gt;: &lt;em&gt;this parameter is not affected by global shadow settings defined via &lt;b&gt;Settings -> Render -> Shadows&lt;/b&gt;.&lt;/em&gt;&lt;/p&gt;">2</state>
		<state name="shadow_penumbra_mode"	transparent="2"	title="Penumbra Mode"	pass_defines="forward"			defines="name_items"		items="none,low,medium,high,ultra"	tooltip="&lt;p&gt;Quality mode to be used for rendering penumbra from all light sources cast on the material.&lt;/p&gt;&lt;p&gt;This mode enables simulation of real-world shadows by keeping sharp contact shadows closer to the base and softening the farther the shadow stretches away.&lt;/p&gt;&lt;p&gt;When disabled shadow edges are crisp and sharp (no shadow softness at all).&lt;/p&gt;&lt;p&gt;&lt;em&gt;Higher&lt;/em&gt; quality produces &lt;em&gt;softer&lt;/em&gt; shadows.&lt;/p&gt;&lt;p&gt;&lt;b&gt;Notice&lt;/b&gt;: &lt;em&gt;this parameter is not affected by global shadow settings defined via &lt;b&gt;Settings -> Render -> Shadows&lt;/b&gt;.&lt;/em&gt;&lt;/p&gt;">0</state>
		<state name="shadow_filter_noise"	transparent="2"	title="Filter Noise"	pass_defines="forward"			defines="name"	shadow_filter_mode="1,2,3,4"					tooltip="&lt;p&gt;Toggles the use of noise for shadow filtering on and off. This noise is used for smoothing shadows cast on the material.&lt;/p&gt;&lt;p&gt;&lt;b&gt;Notice&lt;/b&gt;: &lt;em&gt;this parameter is not affected by global shadow settings defined via &lt;b&gt;Settings -> Render -> Shadows&lt;/b&gt;.&lt;/em&gt;&lt;/p&gt;">1</state>
		<state name="shadow_penumbra_noise"	transparent="2"	title="Penumbra Noise"	pass_defines="forward"			defines="name"	shadow_penumbra_mode="1,2,3,4"					tooltip="&lt;p&gt;Toggles the use of noise for penumbra rendering on and off. This noise is used for smoothing shadows cast on the material.&lt;/p&gt;&lt;p&gt;&lt;b&gt;Notice&lt;/b&gt;: &lt;em&gt;this parameter is not affected by global shadow settings defined via &lt;b&gt;Settings -> Render -> Shadows&lt;/b&gt;.&lt;/em&gt;&lt;/p&gt;">1</state>
		<state name="transparent_shadow"	transparent="2"							pass_defines="shadow,forward"	defines=",TRANSPARENT_SHADOW">0</state>
	</group>
	
	<group name="Refraction">
		<state name="refraction_background_incorrect"	refraction="1" defines="name" pass_defines="refraction"		title="Information Lost Fix"			tooltip="&lt;p&gt;Enables correction of refraction for &quot;information lost&quot; areas using information from areas beyond object's internal volume.&lt;/p&gt;">0</state>
		<state name="refraction_front_side"				refraction="1" refraction_background_incorrect="1" 			title="Front Side Only"					tooltip="&lt;p&gt;Render refraction only for front faces.&lt;/p&gt;&lt;p&gt;This option can be used for glass objects with no cavities inside.  When enabled, the surface shall overlap all transparent objects located behind it.&lt;/p&gt;">0</state>
		<state name="refraction_use_shading_texture"	refraction="1" defines="name"								title="Use Shading Texture (R Channel)"	tooltip="Use the R channel of Shading Texture as a multiplier for IOR">0</state>
	</group>
	
	<!--Parameters-->
	
	<group name="Default">
		<mask24 name="material_mask" deferred="1" shared="0" tooltip="&lt;p&gt;Material bitmask.&lt;/p&gt;&lt;p&gt;The decal will be projected onto a surface having this material assigned, if the material mask of the decal material matches this mask (one bit at least).&lt;/p&gt;">0xffffffff</mask24>
	</group>
	
	<group name="Base">
