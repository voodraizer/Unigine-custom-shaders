	<group name="Noise 2D">
		<slider	name="noise_2d_scale"			transparent="0,1" noise_2d="1"	title="Intensity"		tooltip="&lt;p&gt;Spatial noise color transformation.&lt;/p&gt;">0.5</slider>
		<float4	name="noise_2d_uv_transform"	transparent="0,1" noise_2d="1"	title="UV Transform"	tooltip="&lt;p&gt;Spatial noise coordinates transformation.&lt;/p&gt;">0.1 0.1 0.0 0.0</float4>
	</group>
	
