		<slider	name="parallax_max_layers"			parallax="1" min="1" max="64.0"			title="Max Layers"			tooltip="&lt;p&gt;Parallax mapping maximum steps.&lt;/p&gt;">16</slider>
		<slider	name="parallax_noise"				parallax="1" min="0" max="2.0"			title="Noise"				tooltip="&lt;p&gt;Size of the noise used for parallax mapping.&lt;/p&gt;">1.0f</slider>
		<uv		name="parallax_cutout_uv_transform"	parallax="1" parallax_cutout="1"		title="Cutout UV Transform"	tooltip="&lt;p&gt;Parallax texture coordinates transformation. (X,Y) - scale, (Z,W) - offset.&lt;/p&gt;">1 1 0 0</uv>
	</group>
	
	<!--refraction-->
	<group name="Refraction">
		<slider name="refraction_ior"			transparent="2" refraction="1"	min="1.0"	max="4.0"	expand="1"	title="IOR"			tooltip="&lt;p&gt;Index of refraction&lt;/p&gt;">1.2</slider>
