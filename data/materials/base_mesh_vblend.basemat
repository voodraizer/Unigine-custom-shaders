		<slider	name="metalness"	workflow="0"	shared="0"						tooltip="Metalness multiplier">0.0</slider>
		<slider	name="roughness"	workflow="0"	shared="0"						tooltip="Roughness multiplier">1.0</slider>
		<slider	name="specular"		workflow="0"	shared="0"	title="Specular"	tooltip="Specular multiplier">0.5</slider>
		
		<color	name="diffuse_color"	workflow="1" shared="0"	title="Diffuse"		tooltip="Diffuse color multiplier">1.0 1.0 1.0 1.0</color>
		<color	name="specular_color"	workflow="1" shared="0"	title="Specular"	tooltip="Specular color multiplier">0.0 0.0 0.0 1.0</color>
		<slider	name="gloss"			workflow="1" shared="0"	title="Gloss"		tooltip="Gloss multiplier">0.0</slider>
		
		<slider name="microfiber" tooltip="Microfiber multiplier">0.0</slider>
		
