		<slider	name="emission_triplanar_blend"	emission="1,2" emission_mapping="3,4"									tooltip="&lt;p&gt;Triplanar blending factor for emission textures.&lt;/p&gt;">0.5</slider>
	</group>
	
	<!--lightmap-->
	<group name="Lightmap">
