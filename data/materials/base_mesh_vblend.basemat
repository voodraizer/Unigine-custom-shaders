		<slider name="normal_scale"	max="2.0" max_expand="1"	title="Normal Intensity"	tooltip="Intensity of the normal texture.">1.0</slider>
		<slider name="translucent"									title="Translucent"			tooltip="Translucency multiplier.">0.0</slider>
		
		<slider name="transparent"			transparent="1,2"	max="10.0"	max_expand="1" title="Transparent Multiplier"	tooltip="Transparency multiplier.">1.0</slider>
		<slider name="transparent_pow"		transparent="1,2"	max="2.0"	max_expand="1" title="Transparent Pow"			tooltip="Transparency power.">1.0</slider>
		
		<slider name="opacity_fresnel"		transparent="1,2"	max="1.0"					title="Opacity Fresnel"		tooltip="">0.0</slider>
		<slider name="opacity_fresnel_pow"	transparent="1,2"	max="5.0" max_expand="1"	title="Opacity Fresnel Pow"	tooltip="">2.0</slider>
		
		<uv name="uv_transform"										title="UV Transform"	tooltip="UV transform. (X,Y) - scale, (Z,W) - offset.">1 1 0 0</uv>
