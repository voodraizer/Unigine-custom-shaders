			detail_specular_color="XYZ" detail_gloss="W"
		/>
		
		<uv		name="detail_uv_transform"		detail_mapping="1,2,3,4,5,6"	detail="1"	title="UV Transform"	tooltip="&lt;p&gt;UV transform for detail textures. (X,Y) - scale, (Z,W) - offset.&lt;/p&gt;">1 1 0 0</uv>
		<slider	name="detail_triplanar_blend"	detail_mapping="3,4"			detail="1"							tooltip="&lt;p&gt;Triplanar blending factor for detail textures.&lt;/p&gt;">0.5</slider>
		
		<slider name="detail_visible"			detail="1"	max="2.0"	>1.0</slider>
		<slider name="detail_visible_threshold"	detail="1"				>1.0</slider>
		
		<slider name="detail_albedo_color_visible"	workflow="0"	detail="1"	shared="0">1.0</slider>
		<slider name="detail_metalness_visible"		workflow="0"	detail="1"	shared="0">1.0</slider>
		<slider name="detail_roughness_visible"		workflow="0"	detail="1"	shared="0">1.0</slider>
		
		<slider name="detail_specular_visible"		workflow="0"	detail="1"	shared="0"	>1.0</slider>
		<slider name="detail_microfiber_visible"	workflow="0"	detail="1"	shared="0"	microfiber_map="1"	>1.0</slider>
		
		<slider name="detail_diffuse_color_visible"		workflow="1"	detail="1"	shared="0">0.5</slider>
		<slider name="detail_specular_color_visible"	workflow="1"	detail="1"	shared="0">0.5</slider>
		<slider name="detail_gloss_visible"				workflow="1"	detail="1"	shared="0">0.5</slider>
		
		<combiner name="detail_color_visible" detail="1"
			detail_albedo_color_visible="X"
			detail_diffuse_color_visible="X"
		/>
		
		<combiner name="detail_shading_visible" detail="1"
			detail_metalness_visible="X" detail_roughness_visible="Y" detail_specular_visible="Z" detail_microfiber_visible="W"
			detail_specular_color_visible="XYZ" detail_gloss_visible="W"
		/>
		
		<slider name="detail_normal_visible" detail="1"	>0.5</slider>
		
		<!--detail mask-->
		<uv			name="detail_mask_uv_transform"		detail_mask="1"	detail_mask_mapping=",2,3,4,5,6,7"	detail="1"	title="UV Transform"	tooltip="&lt;p&gt;Detail mask texture coordinates transformation. (X,Y) - scale, (Z,W) - offset.&lt;/p&gt;">1 1 0 0</uv>
		<slider		name="detail_mask_triplanar_blend"	detail_mask="1"	detail_mask_mapping="4,5"			detail="1">0.5f</slider>
		
		<slider name="detail_angle_fade"			detail="1"	detail_angle_fade="1,2">0.5</slider>
		<slider name="detail_angle_fade_threshold"	detail="1"	detail_angle_fade="1,2">0.5</slider>
		
	</group>
	
	<!--emission-->
	<group name="Emission">
