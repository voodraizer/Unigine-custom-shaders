		<texture unit="0" name="albedo"		anisotropy="1" workflow="0" tooltip="&lt;p&gt;Albedo color texture, 4 channels: RGB - color, A - opacity&lt;/p&gt;">core/textures/common/white.dds</texture>
		
		<texture unit="1" name="specular"	anisotropy="1" pass="deferred,transparent,refraction" workflow="1" tooltip="&lt;p&gt;Specular reflection texture, 4 channels: RGB - reflection color, A - glossiness modulation&lt;/p&gt;">core/textures/common/white.dds</texture>
		<texture unit="1" name="shading"	anisotropy="1" pass="deferred,transparent,refraction" workflow="0" tooltip="&lt;p&gt;Shading texture, 4 channels: R - metalness map, G - roughness map, B - specular reflection intensity for non-metals, A - microfiber map&lt;/p&gt;">core/textures/common/white.dds</texture>
		
		<texture unit="2" name="normal"		anisotropy="1" format="signed"	tooltip="&lt;p&gt;Normal texture:&lt;ul&gt;&lt;li&gt;2-channels: RG - normal map&lt;/li&gt;&lt;li&gt;3 channels: RG - normal map, B - opacity (available for transparent objects only, when the &lt;b&gt;Opacity map&lt;/b&gt; is set to &lt;i&gt;Normal&lt;/i&gt;)&lt;/li&gt;&lt;/ul&gt;&lt;/p&gt;&lt;p&gt;This texture is used to fake a relief on a surface instead of using more polygons.&lt;/p&gt;">core/textures/common/normal.dds</texture>
	</group>

	<group name="Blending">
		<texture unit="3" name="albedo_blend_r"		vertex_blend_enable="1"	vertex_color_blend_r="1"		anisotropy="1" 	workflow="0" 	pass="deferred,transparent,refraction" tooltip="&lt;p&gt;Diffuse color texture, 4 channels: RGB - color, A - opacity&lt;/p&gt;">core/textures/common/white.dds</texture>
		<texture unit="4" name="shading_blend_r"	vertex_blend_enable="1"	vertex_color_blend_r="1"		anisotropy="1" 	workflow="0" 	pass="deferred,transparent,refraction" tooltip="&lt;p&gt;Shading texture, 4 channels: R - metalness map, G - roughness map, B - specular reflection intensity for non-metals, A - microfiber map&lt;/p&gt;">core/textures/common/white.dds</texture>
		<texture unit="5" name="normal_blend_r"		vertex_blend_enable="1"	vertex_color_blend_r="1"		anisotropy="1"	workflow="0" 	pass="deferred,transparent,refraction" format="signed"	tooltip="&lt;p&gt;Normal texture:&lt;ul&gt;&lt;li&gt;2-channels: RG - normal map&lt;/li&gt;&lt;li&gt;3 channels: RG - normal map, B - opacity (available for transparent objects only, when the &lt;b&gt;Opacity map&lt;/b&gt; is set to &lt;i&gt;Normal&lt;/i&gt;)&lt;/li&gt;&lt;/ul&gt;&lt;/p&gt;&lt;p&gt;This texture is used to fake a relief on a surface instead of using more polygons.&lt;/p&gt;">core/textures/common/normal.dds</texture>

		<texture unit="7" name="albedo_blend_g"	 	vertex_blend_enable="1"	vertex_color_blend_g="1"		anisotropy="1" 	workflow="0" 	pass="deferred,transparent,refraction" tooltip="&lt;p&gt;Diffuse color texture, 4 channels: RGB - color, A - opacity&lt;/p&gt;">core/textures/common/white.dds</texture>		
		<texture unit="8" name="shading_blend_g"  	vertex_blend_enable="1"	vertex_color_blend_g="1"		anisotropy="1" 	workflow="0" 	pass="deferred,transparent,refraction" tooltip="&lt;p&gt;Shading texture, 4 channels: R - metalness map, G - roughness map, B - specular reflection intensity for non-metals, A - microfiber map&lt;/p&gt;">core/textures/common/white.dds</texture>
		<texture unit="9" name="normal_blend_g"	 	vertex_blend_enable="1"	vertex_color_blend_g="1"		anisotropy="1" 	workflow="0" 	pass="deferred,transparent,refraction" format="signed"	tooltip="&lt;p&gt;Normal texture:&lt;ul&gt;&lt;li&gt;2-channels: RG - normal map&lt;/li&gt;&lt;li&gt;3 channels: RG - normal map, B - opacity (available for transparent objects only, when the &lt;b&gt;Opacity map&lt;/b&gt; is set to &lt;i&gt;Normal&lt;/i&gt;)&lt;/li&gt;&lt;/ul&gt;&lt;/p&gt;&lt;p&gt;This texture is used to fake a relief on a surface instead of using more polygons.&lt;/p&gt;">core/textures/common/normal.dds</texture>
	</group>

	<group name="Blending mask">
		<texture unit="6" name="mask_blend"	 		vertex_blend_enable="1"	anisotropy="1" 	workflow="0" 	pass="deferred,transparent,refraction" tooltip="&lt;p&gt;Diffuse color texture, 4 channels: RGB - color, A - opacity&lt;/p&gt;">core/textures/common/white.dds</texture>
	</group>
	
	<group name="Detail">
		<texture unit="3"	name="detail_diffuse"	detail="1" anisotropy="1" pass="deferred,transparent" workflow="1"		title="Diffuse"	tooltip="&lt;p&gt;Additional diffuse texture, 4 channels: RGB - color, A - opacity.&lt;/p&gt;&lt;p&gt;Can be used to add details for objects, that are close to camera, or variations for distant ones, depending on UV transform settings.&lt;/p&gt;">core/textures/common/white.dds</texture>
		<texture unit="3"	name="detail_albedo"	detail="1" anisotropy="1" pass="deferred,transparent" workflow="0"		title="Albedo"	tooltip="&lt;p&gt;Additional albedo texture, 4 channels: RGB - color, A - opacity.&lt;/p&gt;&lt;p&gt;Can be used to add details for objects, that are close to camera, or variations for distant ones, depending on UV transform settings.&lt;/p&gt;">core/textures/common/white.dds</texture>
		
		<texture unit="4"	name="detail_specular"	detail="1" anisotropy="1" pass="deferred,transparent" workflow="1"		title="Specular"	tooltip="&lt;p&gt;Additional specular reflection texture, 4 channels: RGB - reflection color, A - glossiness modulation&lt;/p&gt;&lt;p&gt;Can be used to add details for objects, that are close to camera, or variations for distant ones, depending on UV transform settings.&lt;/p&gt;">core/textures/common/white.dds</texture>
		<texture unit="4"	name="detail_shading"	detail="1" anisotropy="1" pass="deferred,transparent" workflow="0"		title="Shading"		tooltip="&lt;p&gt;Additional shading texture, 4 channels: R - metalness map, G - roughness map, B - specular reflection intensity for non-metals, A - microfiber map&lt;/p&gt;&lt;p&gt;Can be used to add details for objects, that are close to camera, or variations for distant ones, depending on UV transform settings.&lt;/p&gt;">core/textures/common/white.dds</texture>
		
		<texture unit="5"	name="detail_normal"	detail="1" anisotropy="1" pass="deferred,transparent" format="signed"		title="Normal"		tooltip="&lt;p&gt;Additional normal texture, 2 channels: RG - normal map.&lt;/p&gt;&lt;p&gt;Can be used to add details for objects, that are close to camera, or variations for distant ones, depending on UV transform settings.&lt;/p&gt;">core/textures/common/normal.dds</texture>
		<texture unit="6"	name="detail_mask"		detail="1" anisotropy="1" pass="deferred,transparent" detail_mask="1"		title="Blend Mask"	tooltip="&lt;p&gt;Detail blend mask texture, 1 channel: R - blending map.&lt;/p&gt;&lt;p&gt;Determines how base and detail textures are to be blended:&lt;ul&gt;&lt;li&gt;&lt;b&gt;black&lt;/b&gt; - only base textures are shown&lt;/li&gt;&lt;li&gt;&lt;b&gt;red&lt;/b&gt; - only detail textures are shown&lt;/li&gt;&lt;/ul&gt;&lt;/p&gt;">core/textures/mesh_layer_mask.dds</texture>
	</group>
	
	<group name="Options">
		<texture unit="7"	name="ambient_occlusion"	anisotropy="1" pass="deferred,forward"			ao_map="1"		title="Ambient Occlusion"	tooltip="&lt;p&gt;Ambient occlusion texture, 1 channel: R - AO map.&lt;/p&gt;&lt;p&gt;This texture is used to modulate global environment illumination and contains baked shadows from environment probe light sources.&lt;/p&gt;">core/textures/common/red.dds</texture>
		<texture unit="8"	name="lightmap"				anisotropy="1" pass="deferred,ambient"			lightmap="1"				tooltip="&lt;p&gt;Lightmap texture, 3 channels: RGB - light color.&lt;/p&gt;&lt;p&gt;This texture contains pre-computed light information and is usually used for static objects.&lt;/p&gt;">core/textures/common/white.dds</texture>
		<texture unit="12"	name="procedural_effects"	anisotropy="1" pass="deferred"					procedural_effects="1" 		tooltip="&lt;p&gt;Texture used for procedural effects, 3 channels: R - bevel radius (SSBevel), G - cavity mask (SSDirt), B - convexity mask (SSDirt)&lt;/p&gt;&lt;p&gt;This texture combines maps for the SSBevel and SSDirt effects.&lt;/p&gt;">core/textures/procedural_effects.dds</texture>
		<texture unit="9"	name="microfiber"			anisotropy="1" pass="deferred,transparent"		workflow="1"				tooltip="&lt;p&gt;Microfiber texture, 1 channel: R - microfiber map.&lt;/p&gt;">core/textures/common/red.dds</texture>
		
		<texture unit="3"	name="auxiliary"	anisotropy="1" pass="auxiliary"	auxiliary="1"	tooltip="&lt;p&gt;Auxiliary texture, 4 channels.&lt;/p&gt;&lt;p&gt;This texture can be used for custom effects, such as thermal vision, etc.&lt;/p&gt;">core/textures/common/white.dds</texture>
		
		<texture unit="10"	name="parallax"		anisotropy="1" pass="deferred,emission,shadow,depth_pre_pass"	parallax="1,2"	transparent="0,1"	tooltip="&lt;p&gt;Parallax height map texture, 1 channel: R - height map&lt;/p&gt;&lt;p&gt;This texture can be used to fake surface relief adding more realism with less influence on the performance.&lt;/p&gt;">core/textures/common/red.dds</texture>
		
		<texture unit="11"	name="translucent"	anisotropy="1" pass="deferred"	translucent_map="1"	transparent="0,1"	tooltip="&lt;p&gt;Translucency texture, 1 channel: R - translucency color.&lt;/p&gt;&lt;p&gt;This texture is used to specify parts of the object, that allow light to pass through.&lt;/p&gt;">core/textures/common/red.dds</texture>
		
		<texture unit="13"	name="noise_2d"		anisotropy="1" pass="deferred"			noise_2d="1"	transparent="0,1"	title="2D Noise"	tooltip="&lt;p&gt;Spatial color noise texture, 3 channels: RGB - color.&lt;/p&gt;&lt;p&gt;Can be used to add more realism due to color variation (e.g., for a forest). Blended in overlay mode.&lt;/p&gt;">core/textures/leaf/noise_2d.dds</texture>
		<texture unit="13"	name="emission"		anisotropy="1" pass="emission,ambient"	emission="1"											tooltip="&lt;p&gt;Ambient emission texture, 3 channels: RGB - emission color.&lt;/p&gt;&lt;p&gt;This texture is used for self-luminous objects (lamps, fire, etc.).&lt;/p&gt;">core/textures/common/white.dds</texture>
	</group>
	
	<group name="3D Noise">
		<texture unit="14"	pass="deferred" name="noise_3d"				noise_3d="1" transparent="0,1" anisotropy="1"	title="3D Noise"		tooltip="&lt;p&gt;Color noise texture, 1 channel: R - noise.&lt;/p&gt;&lt;p&gt;This texture can be used to randomize coloring of leaves according to the gradient color texture.&lt;/p&gt;">core/textures/leaf/noise_3d.dds</texture>
		<texture unit="15"	pass="deferred" name="noise_3d_gradient"	noise_3d="1" transparent="0,1" wrap="clamp"	title="Color gradient"	tooltip="&lt;p&gt;Color gradient texture, 3 channels: RGB - color.&lt;/p&gt;&lt;p&gt;A gradient texture that modulates colors of leaves (for example, from bright green to yellow).&lt;/p&gt;">core/textures/leaf/color.dds</texture>
	</group>
	
	<texture unit="10"	type="reflection_cube"			pass="ambient"/>
	<texture unit="11"	type="transparent_environment"	pass="ambient"				planar_reflection="0"/>
	<texture unit="11"	type="reflection_2d"			pass="ambient,reflection"	planar_reflection="1"/>
	<texture unit="12"	type="scattering_sky_lut"		pass="ambient"/>
	<texture unit="14"	type="gbuffer_normal"			pass="reflection"			planar_reflection="1"/>
	<texture unit="14"	type="light_image"				pass="forward"/>
	<texture unit="15"	type="light_shadow_depth"		pass="forward"/>
	<texture unit="17"	type="light_shadow_color"		pass="forward"/>
	
	<texture unit="16"	name="internal_noise" filter="point"	hidden="1">core/textures/common/noise.dds</texture>
	
	<!--
	Passes
	-->
	<!--deferred-->
		<shader pass="deferred" node="object_mesh_static"
			deferred="1"
			vertex="shaders/mesh_vblend/base_mesh_vblend.vert"
			fragment="shaders/mesh_vblend/base_mesh_vblend.frag"/>
		
		<shader pass="depth_pre_pass" node="object_mesh_static"
			deferred="1"
			defines="BASE_ALPHA_TEST"
			vertex="core/shaders/mesh/depth_pre_pass.shader"
			fragment="core/shaders/mesh/depth_pre_pass.shader"/>
		
		<shader pass="reflection" node="object_mesh_static"
			transparent="0,1"
			deferred="1"
			planar_reflection="1"
			defines="BASE_REFLECTION"
			vertex="core/shaders/mesh/opacity/reflection.shader"
			fragment="core/shaders/mesh/opacity/reflection.shader"/>
		
	<!---->
	
	<!--transparent passes-->
		
		<!--forward passes-->
			<shader pass="ambient" node="object_mesh_static"
				transparent="2"
				ambient="1"
				defines="TRANSPARENT"
				light_world_defines=",LIGHT_WORLD"
				ambient_defines=",AMBIENT"
				vertex="core/shaders/mesh/transparent/ambient.shader"
				fragment="core/shaders/mesh/transparent/ambient.shader"/>
			<shader pass="ambient" node="object_mesh_static"
				transparent="2"
				ambient="0"
				light_world="1"
				defines="TRANSPARENT"
				light_world_defines=",LIGHT_WORLD"
				ambient_defines=",AMBIENT"
				vertex="core/shaders/mesh/transparent/ambient.shader"
				fragment="core/shaders/mesh/transparent/ambient.shader"/>
			<shader pass="ambient" node="object_mesh_static"
				transparent="2"
				ambient="0"
				light_world="0"
				emission="1"
				defines="TRANSPARENT"
				light_world_defines=",LIGHT_WORLD"
				ambient_defines=",AMBIENT"
				vertex="core/shaders/mesh/transparent/ambient.shader"
				fragment="core/shaders/mesh/transparent/ambient.shader"/>
			
			<shader pass="light_omni" node="object_mesh_static"
				transparent="2"
				light_omni="1"
				defines="OMNI,TRANSPARENT"
				vertex="core/shaders/mesh/transparent/lights.shader"
				fragment="core/shaders/mesh/transparent/lights.shader"/>
			
			<shader pass="light_proj" node="object_mesh_static"
				transparent="2"
				light_proj="1"
				defines="PROJ,TRANSPARENT"
				vertex="core/shaders/mesh/transparent/lights.shader"
				fragment="core/shaders/mesh/transparent/lights.shader"/>
			
			<shader pass="light_world" node="object_mesh_static"
				transparent="2"
				light_world="1"
				defines="WORLD,TRANSPARENT"
				vertex="core/shaders/mesh/transparent/lights.shader"
				fragment="core/shaders/mesh/transparent/lights.shader"/>
			
			<shader pass="light_environment_probe" node="object_mesh_static"
				transparent="2"
				ambient="1"
				environment_probe="1"
				defines="PROBE,TRANSPARENT"
				vertex="core/shaders/mesh/transparent/light_probe.shader"
				fragment="core/shaders/mesh/transparent/light_probe.shader"/>
				
			<shader pass="light_voxel_probe" node="object_mesh_static"
				transparent="2"
				ambient="1"
				voxel_probe="1"
				defines="VOXEL_PROBE,TRANSPARENT"
				vertex="core/shaders/mesh/transparent/light_voxel_probe.shader"
				fragment="core/shaders/mesh/transparent/light_voxel_probe.shader"/>
		<!---->
		
		<shader pass="refraction" node="object_mesh_static"
			transparent="2"
			refraction="1"
			defines="TRANSPARENT"
			vertex="core/shaders/mesh/transparent/refraction.shader"
			fragment="core/shaders/mesh/transparent/refraction.shader"/>
		
		<shader pass="transparent_blur" node="object_mesh_static"
			transparent="2"
			transparent_blur="1"
			defines="TRANSPARENT"
			vertex="core/shaders/mesh/transparent/transparent_blur.shader"
			fragment="core/shaders/mesh/transparent/transparent_blur.shader"/>
	<!---->
	
	<shader pass="shadow" node="object_mesh_static"
		defines="BASE_SHADOW"
		vertex="core/shaders/mesh/shadow.shader"
		fragment="core/shaders/mesh/shadow.shader"/>
	
	<shader pass="auxiliary" node="object_mesh_static"
		auxiliary="1"
		defines="BASE_AUXILIARY"
		vertex="core/shaders/mesh/auxiliary/auxiliary.shader"
		fragment="core/shaders/mesh/auxiliary/auxiliary.shader"/>
	
	<shader pass="emission" node="object_mesh_static"
		transparent="0,1"
		emission="1"
		defines="BASE_EMISSION,EMISSION"
		vertex="core/shaders/mesh/emission/emission.shader"
		fragment="core/shaders/mesh/emission/emission.shader"/>
	
	<shader pass="wireframe" node="object_mesh_static"
		defines="BASE_WIREFRAME"
		vertex="core/shaders/mesh/common/default.vert"
		geometry="core/shaders/common/wireframe/wireframe.geom"
		fragment="core/shaders/common/wireframe/wireframe.frag"/>
	
	<shader pass="visualizer_solid" node="object_mesh_static"
		defines="BASE_VISUALIZER_SOLID"
		vertex="core/shaders/mesh/common/default.vert"
		fragment="core/shaders/common/wireframe/wireframe.frag"/>
	
	<!--
	Bindings
	-->
	<bind node="object_mesh_cluster" to="object_mesh_static" defines="USE_CLUTTER_CLUSTER_PARAMETERS"/>
	<bind node="object_mesh_clutter" to="object_mesh_static" defines="USE_CLUTTER_CLUSTER_PARAMETERS"/>
	<bind node="object_mesh_dynamic" to="object_mesh_static"/>
	<bind node="object_mesh_skinned" to="object_mesh_static" defines="SKINNED"/>
	<bind node="object_mesh_spline_cluster" to="object_mesh_static" defines="USE_CLUTTER_CLUSTER_PARAMETERS"/>
	
</base_material>