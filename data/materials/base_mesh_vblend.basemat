		<slider name="triplanar_blend"	base_mapping="2,3"							title="Triplanar Blend"	tooltip="Triplanar blending factor.">0.5</slider>
		<slider name="mip_bias"			mip_bias="1"		min="-8.0" max="8.0"	title="Mip Bias"		tooltip="">0.0</slider>

	</group>

	<group name="Blending">
		<slider	name="blend_scale_uv_2"		vertex_blend_enable="1"		vertex_color_blend_r="1"								min="0.0" max="10.0" workflow="0"					title="UV scale 2nd set"					tooltip="UV scale 2nd texture set.">1.0</slider>
		<slider	name="blend_scale_uv_4"		vertex_blend_enable="1"									vertex_color_blend_g="1"	min="0.0" max="10.0" workflow="0"					title="UV scale 3rd set"					tooltip="UV scale 3nd texture set.">1.0</slider>
		<slider	name="blend_scale_uv_3"		vertex_blend_enable="1"																min="0.0" max="10.0" workflow="0"					title="UV scale mask"						tooltip="UV scale mask.">1.0</slider>

		<slider	name="blend_factor_r"		vertex_blend_enable="1"		vertex_color_blend_r="1"								min="0.0" max="20.0" workflow="0"					title="Blend factor (R)"					tooltip="Blend factor.">1.5</slider>
		<slider	name="blend_falloff_r"		vertex_blend_enable="1"		vertex_color_blend_r="1"								min="0.0" max="20.0" workflow="0"					title="Blend fallof (R)"					tooltip="Blend falloff.">2.0</slider>
		<slider name="blend_alpha_r"		vertex_blend_enable="1"		vertex_color_blend_r="1" 								min="0.0" max="20.0" workflow="0"	max_expand="1" 	title="Blend alpha (R)"		tooltip="Blend alpha.">5.0</slider>
		<slider name="blend_normals_r" 		vertex_blend_enable="1"		vertex_color_blend_r="1" 								min="0.0" max="1.0"  workflow="0"	max_expand="1" 	title="Blend normals (R)"	tooltip="Blend normals.">0.7</slider>

		<slider	name="blend_factor_g"		vertex_blend_enable="1"									vertex_color_blend_g="1"	min="0.0" max="20.0" workflow="0"					title="Blend factor (G)"					tooltip="Blend factor.">1.5</slider>
		<slider	name="blend_falloff_g"		vertex_blend_enable="1"									vertex_color_blend_g="1"	min="0.0" max="20.0" workflow="0"					title="Blend fallof (G)"					tooltip="Blend falloff.">1.5</slider>
		<slider name="blend_alpha_g" 		vertex_blend_enable="1"									vertex_color_blend_g="1"	min="0.0" max="20.0" workflow="0"	max_expand="1" 	title="Blend alpha (G)"		tooltip="Blend alpha.">3.0</slider>
		<slider name="blend_normals_g" 		vertex_blend_enable="1"									vertex_color_blend_g="1"	min="0.0" max="1.0"  workflow="0"	max_expand="1" 	title="Blend normals (G)"	tooltip="Blend normals.">1.0</slider>

		<slider name="dirt_alpha" 			vertex_blend_enable="1"																min="0.0" max="1.0"  workflow="0"	max_expand="1" 	title="Dirt alpha"			tooltip="Dirt alpha.">0.3</slider>
		<slider name="dirt_roughness_coeff" vertex_blend_enable="1"																min="0.0" max="2.0"  workflow="0"	max_expand="1" 	title="Dirt roughness"		tooltip="Dirt roughness coefficient">0.5</slider>
	</group>
	
	<group name="Detail">
		<color	name="detail_albedo_color"	detail="1"	workflow="0"	shared="0"	title="Albedo"		tooltip="Detail albedo color multiplier.">1.0 1.0 1.0 1.0</color>
		<slider	name="detail_metalness"		detail="1"	workflow="0"	shared="0"	title="Metalness"	tooltip="Detail metalness multiplier.">0.0</slider>
		<slider	name="detail_roughness"		detail="1"	workflow="0"	shared="0"	title="Roughness"	tooltip="Detail roughness multiplier.">1.0</slider>
		
		<slider	name="detail_specular"		detail="1"	workflow="0"	shared="0"	title="Specular"	tooltip="Detail specular multiplier.">0.5</slider>
		<slider	name="detail_microfiber"	detail="1"	workflow="0"	shared="0"	microfiber_map="1"	title="Microfiber"	tooltip="Detail microfiber multiplier.">0.0</slider>
		
		<color	name="detail_diffuse_color"		detail="1"	workflow="1"	shared="0"	title="Diffuse"		tooltip="Detail diffuse color multiplier.">1.0 1.0 1.0 1.0</color>
		<color	name="detail_specular_color"	detail="1"	workflow="1"	shared="0"	title="Specular"	tooltip="Detail specular color multiplier.">0.0 0.0 0.0 1.0</color>
		<slider	name="detail_gloss"				detail="1"	workflow="1"	shared="0"	title="Gloss"		tooltip="Detail gloss multiplier.">0.0</slider>
		
		<combiner name="detail_color" detail="1"
			detail_albedo_color="XYZW"
			detail_diffuse_color="XYZW"
