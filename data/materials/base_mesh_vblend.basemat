	<group name="SSBevel">
		<slider	name="bevel_scale"				bevel="1"						max_expand="1"	title="Scale"					tooltip="&lt;p&gt;Scale factor for the SSBevel effect.&lt;/p&gt;&lt;p&gt;Higher values result in more pronounced effect.&lt;/p&gt;">1.0</slider>
		<uv		name="bevel_uv_transform"		bevel="1"										title="SSBevel UV Transform"	tooltip="&lt;p&gt;UV-coordinates transformation for SSBevel map. (X,Y) - scale, (Z,W) - offset.&lt;/p&gt;">1 1 0 0</uv>
		<slider	name="bevel_triplanar_blend"	bevel="1"	bevel_mapping="2"	max_expand="1"	title="Triplanar Blend"			tooltip="Triplanar blending factor.">0.5</slider>
	</group>
	
	<!--SSDirt-->
	<group name="SSDirt">
		<slider	name="curvature_scale_cavity"		curvature="1"	max_expand="1"	title="Cavity Scale"			tooltip="&lt;p&gt;Scale factor for the SSDirt effect in concave areas.&lt;/p&gt;&lt;p&gt;Higher values result in more pronounced effect in slits and cavities.&lt;/p&gt;">1.0</slider>
		<slider	name="curvature_scale_convexity"	curvature="1"	max_expand="1"	title="Convexity Scale"			tooltip="&lt;p&gt;Scale factor for the SSDirt effect in convex areas.&lt;/p&gt;&lt;p&gt;Higher values result in more pronounced effect on edges and convexities.&lt;/p&gt;">1.0</slider>
		<uv		name="curvature_uv_transform"		curvature="1"					title="Curvature UV Transform"	tooltip="&lt;p&gt;UV-coordinates transformation for textures applied in concave and convex areas. (X,Y) - scale, (Z,W) - offset.&lt;/p&gt;">1 1 0 0</uv>
	</group>
	
