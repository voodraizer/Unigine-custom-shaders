		<slider	name="reflection_distance_scale"	shared="0" planar_reflection="1" title="Distance Scale"	min="0.0" max="10.0"	tooltip="Distance scale for all distance parameters: shadows distance, LOD distances, etc. Overrides the render_distance_scale while rendering planar reflections">1.0</slider>
		<float3	name="reflection_pivot_rotation"	shared="0" planar_reflection="1" title="Pivot Rotation"							tooltip="&lt;p&gt;Rotation of the reflection pivot point.&lt;/p&gt;&lt;p&gt;Can be used to correct reflection in case if the transformation of reflective surface was changed.&lt;/p&gt;">0 0 0</float3>
		<float3	name="reflection_pivot_offset"		shared="0" planar_reflection="1" title="Pivot Offset"							tooltip="&lt;p&gt;Position of the reflection pivot point.&lt;/p&gt;&lt;p&gt;Can be used to correct reflection in case if the transformation of reflective surface was changed.&lt;/p&gt;">0 0 0</float3>
